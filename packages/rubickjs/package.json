{
  "name": "rubickjs",
  "version": "0.0.1-beta.1",
  "description": "Rubick.js is a JavaScript game and rendering engine",
  "author": "wxm",
  "license": "MIT",
  "keywords": [
    "rubick",
    "rubickjs",
    "webgl",
    "webgl2",
    "game-engine",
    "2d"
  ],
  "homepage": "https://github.com/qq15725/rubickjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qq15725/rubickjs.git"
  },
  "bugs": {
    "url": "https://github.com/qq15725/rubickjs/issues"
  },
  "scripts": {
    "build": "pnpm run clean && rollup -c",
    "clean": "rimraf dist",
    "watch": "rollup -c -w"
  },
  "files": [
    "package.json",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.js"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "unpkg": "dist/index.umd.min.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@rubickjs/assets": "workspace:*",
    "@rubickjs/input": "workspace:*",
    "@rubickjs/math": "workspace:*",
    "@rubickjs/renderer": "workspace:*",
    "@rubickjs/scene": "workspace:*",
    "@rubickjs/shared": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
